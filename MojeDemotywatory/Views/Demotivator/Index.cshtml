@model MojeDemotywatory.Models.PageModel

@{
    ViewBag.Title = "Index";
}
<div class="demotivator-center">

    @foreach (var d in @Model.DemotivatorList)
    {
        <table>
            <tr>

                <td>
                    <a href="@d.Url">
                        <img onload="" src="@d.ImgUrl"/>
                    </a>
                </td>

            </tr>
            <tr>
                <td>
                    <a class="btn btn-primary" href="@Url.Action("Dodaj", "Fajne", new {adres = @d.ImgUrl})">Ten jest fajny!</a>
                </td>
            </tr>
        </table>



        <br/>

    }
    @foreach (var s in @Model.DemotivatorSlideList)
    {
        <table>
            <tr>

                <td><img src="@s.ImgUrl"/></td>
                <td class="td2">@s.Description</td>

            </tr>
            <tr>
                <td>
                    <a class="btn btn-primary" href="@Url.Action("Dodaj", "Fajne", new {adres = @s.ImgUrl})">Ten jest fajny!</a>
                </td>
            </tr>
        </table>
    }

</div>

<script>

  
    var image = document.images[0];
    var downloadingImage = new Image();
    downloadingImage.onload = function () {
        image.src = this.src;
    };
    downloadingImage.src = "http://an.image/to/aynchrounously/download.jpg";

</script>